{
  "name": "Hello World",
  "description": "This extension shows a Google Image search result",
  "version": "1.0",
  "minimum_chrome_version": "30.0",
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "manifest_version": 2
}